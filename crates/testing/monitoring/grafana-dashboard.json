{
  "dashboard": {
    "id": null,
    "title": "SIS Kernel Industry-Grade Testing Dashboard",
    "tags": ["sis-kernel", "testing", "performance", "security"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["1s", "5s", "10s", "30s", "1m", "5m"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Overall Test Score",
        "type": "stat",
        "targets": [
          {
            "expr": "sis_kernel_overall_score_percent",
            "legendFormat": "Overall Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 90}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Test Coverage by Category",
        "type": "bargauge",
        "targets": [
          {
            "expr": "sis_kernel_performance_coverage_percent",
            "legendFormat": "Performance"
          },
          {
            "expr": "sis_kernel_correctness_coverage_percent",
            "legendFormat": "Correctness"
          },
          {
            "expr": "sis_kernel_security_coverage_percent",
            "legendFormat": "Security"
          },
          {
            "expr": "sis_kernel_distributed_coverage_percent",
            "legendFormat": "Distributed"
          },
          {
            "expr": "sis_kernel_ai_coverage_percent",
            "legendFormat": "AI/ML"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 95}
              ]
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "basic"
        },
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "AI Inference Latency (P99)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sis_kernel_ai_inference_latency_p99_microseconds",
            "legendFormat": "P99 Latency"
          },
          {
            "expr": "40",
            "legendFormat": "Target (40μs)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "µs",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "thresholdsStyle": {
                "mode": "line"
              }
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 40}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Context Switch Latency (P95)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sis_kernel_context_switch_latency_p95_nanoseconds",
            "legendFormat": "P95 Latency"
          },
          {
            "expr": "500",
            "legendFormat": "Target (500ns)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ns",
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 500}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Byzantine Consensus Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sis_kernel_byzantine_consensus_latency_milliseconds",
            "legendFormat": "Consensus Latency"
          },
          {
            "expr": "5",
            "legendFormat": "Target (5ms)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "System Throughput",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(sis_kernel_throughput_ops_total[1m])",
            "legendFormat": "Throughput (ops/sec)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 7,
        "title": "Memory Safety Violations",
        "type": "stat",
        "targets": [
          {
            "expr": "sis_kernel_memory_safety_violations_total",
            "legendFormat": "Violations"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "red", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24}
      },
      {
        "id": 8,
        "title": "Critical Vulnerabilities",
        "type": "stat",
        "targets": [
          {
            "expr": "sis_kernel_vulnerabilities_critical_total",
            "legendFormat": "Critical"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "red", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 24}
      },
      {
        "id": 9,
        "title": "AI Inference Accuracy",
        "type": "stat",
        "targets": [
          {
            "expr": "sis_kernel_ai_inference_accuracy_percent",
            "legendFormat": "Accuracy"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 99,
            "max": 100,
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 99},
                {"color": "yellow", "value": 99.9},
                {"color": "green", "value": 99.95}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 24}
      },
      {
        "id": 10,
        "title": "Neural Engine Utilization",
        "type": "gauge",
        "targets": [
          {
            "expr": "sis_kernel_neural_engine_utilization_percent",
            "legendFormat": "Utilization"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 90}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 24}
      }
    ]
  },
  "overwrite": true
}